# Docker 환경 변수 설정 예시
# 이 파일을 .env로 복사하여 사용하세요

# MySQL 데이터베이스 설정
MYSQL_ROOT_PASSWORD=household_ledger_root_password
MYSQL_DATABASE=household_ledger
MYSQL_USER=household_user
MYSQL_PASSWORD=household_password

# Redis 설정 (향후 사용)
REDIS_PASSWORD=household_redis_password

# 타임존 설정
TZ=Asia/Seoul

# 포트 설정 (필요시 변경)
MYSQL_PORT=3306
PHPMYADMIN_PORT=8080
REDIS_PORT=6379

# 개발 환경 설정
COMPOSE_PROFILES=dev  # dev 프로필 기본 활성화 (phpMyAdmin 포함)
